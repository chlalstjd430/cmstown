<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cmstown.mybatis.AccountMapper">

    <!--  댓글 목록  -->
    <select id="selectReplyPassword" parameterType="Integer" resultType="String">
       select password
        from reply_tb
        where reply_id = #{replyID}
    </select>



